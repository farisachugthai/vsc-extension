    "glob": "^7.1.5",
    "gulp": "^4.0.2",
    "mocha": "^6.2.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "vsce": "^1.74.0",
    "vscode-test": "^1.2.2"
  },
  "displayName": "IPython for VSCode",
  "engineStrict": true,
  "engines": {
